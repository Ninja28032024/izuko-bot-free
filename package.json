    "googleapis": "^144.0.0",
    "megajs": "^1.3.9",
    "moment-timezone": "^0.5.45",
    "node-cache": "^5.1.2",
    "node-cleanup": "^2.1.2",
    "node-cron": "^3.0.3",
    "node-webpmux": "^3.2.0",
    "pino": "^9.4.0",
    "sharp": "^0.33.5"
  },
